<div class="jumbotron">
  <h1>Your Profile</h1>
</div>
<hr>
<a href="editProfile">Edit</a>
<hr>
<div class="container">
  <div class="col-sm-6">
    <img src="<%= user.imageURL %>" alt="image of user">
  </div>
  <div class="col-sm-6">
      <table class="table table-bordered">
        <tbody>
          <tr>
            <td>googlename</td>
            <td><%= user.googlename %> </td>
          </tr>
          <tr>
            <td>googleemail</td>
            <td><%= user.googleemail %> </td>
          </tr>
          <tr>
            <td>username</td>
            <td><%= user.username %> </td>
          </tr>
          <tr>
            <td>age</td>
            <td><%= user.age %> </td>
          </tr>
        </tbody>
      </table>
      <ul>
        <li><a href="/showMeeting">See your meetings</a></li>
      </ul>
  </div>
</div>
